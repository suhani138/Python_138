
lst = [10, 10.5, "Apple", 34, 47]

while True:
    print("Enter a number to perform a specific operation on the list:")
    print("1. Print the list using a for loop")
    print("2. Print the list using a for loop with range")
    print("3. Print the list using a while loop")
    print("4. Print the list using list comprehension")
    print("5. Print the list using enumeration with if else")
    print("6. Exit")
    choice = int(input("Enter your choice: "))
    
    if choice == 1:
        for item in lst:
            print(item)
    elif choice == 2:
        for i in range(len(lst)):
            print(lst[i])
    elif choice == 3:
        i = 0
        while i < len(lst):
            print(lst[i])
            i += 1
    elif choice == 4:
        new_lst = [item for item in lst]
        print(new_lst)
    elif choice == 5:
        for i, item in enumerate(lst):
            if i % 2 == 0:
                print(item)
            else:
                print(item * 2)
    elif choice == 6:
        break
    else:
        print("Invalid choice. Please try again.")


d = {'key 1': 1, 'key 2': 2, 'key 3': 3}

while True:
    print("Enter a number to perform a specific operation on the dictionary:")
    print("1. Print the dictionary using a for loop with keys")
    print("2. Print the dictionary using a for loop with values")
    print("3. Print the dictionary using a for loop with items")
    print("4. Exit")
    choice = int(input("Enter your choice: "))
    
    if choice == 1:
        for key in d.keys():
            print(key)
    elif choice == 2:
        for value in d.values():
            print(value)
    elif choice == 3:
        for key, value in d.items():
            print(key, value)
    elif choice == 4:
        break
    else:
        print("Invalid choice. Please try again.")
